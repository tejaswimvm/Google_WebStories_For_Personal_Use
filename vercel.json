{
  "rewrites": [
    {
      "source": "/webstories/editor.html",
      "destination": "/editor.html"
    },
    {
      "source": "/webstories/index.html",
      "destination": "/index.html"
    },
    {
      "source": "/webstories",
      "destination": "/index.html"
    },
    {
      "source": "/webstories/",
      "destination": "/index.html"
    },
    {
      "source": "/webstories/(.*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}
